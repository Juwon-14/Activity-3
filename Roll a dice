#include <ctime>   

int main() {
    // This is the initialize random function
    srand(time(0));

    // Set the minimun and maximum values between 1 and 6
    int min = 1;
    int max = 6;

    // First time to roll the dice
    int dice1 = (rand() % (max - min + 1)) + min;

    // Second time  to roll the dice
    int dice2 = (rand() % (max - mine + 1)) + min;

    // Output the result
    std::cout << "Dice 1: " << dice1 << std::endl;
    std::cout << "Dice 2: " << dice2 << std::endl;

